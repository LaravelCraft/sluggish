{
  "name": "laravel-craft/sluggish",
  "description": "A complete solution for generating slug, uuid and unique number for any laravel projects.",
  "type": "library",
  "homepage": "https://github.com/LaravelCraft/sluggish.git",
  "keywords": [
    "sluggish",
    "laravel-craft-sluggish",
    "unique-slug-helper",
    "laravel-slug-helper",
    "slug-generator",
    "unique-slug-generator",
    "laravel-unique-slug-generator"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LaravelCraft\\Sluggish\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelCraft\\Sluggish\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Akramul Hoque",
      "email": "akramulhoque.engineer@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {},
  "extra": {
    "laravel": {
      "providers": [
        "LaravelCraft\\Sluggish\\SluggishServiceProvider"
      ],
      "aliases": {
        "Sluggish": "LaravelCraft\\Sluggish\\Facades\\SluggishFacade"
      }
    }
  }
}
